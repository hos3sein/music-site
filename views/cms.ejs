  crossorigin="anonymous"></script>

    <title>home</title>
</head>

<body  >

  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <div class="container-fluid">
      <a onclick="fn('music')" class="navbar-brand" href="#">music</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarText">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a onclick="fn('photo')" class="nav-link active" aria-current="page" href="#">photo</a>
          </li>
          <li class="nav-item">
            <a onclick="fn('txt')" class="nav-link" href="#">text</a>
          </li>
          <li class="nav-item">
            <a onclick="fn('user')" class="nav-link" href="#">users</a>
          </li>
          <li class="nav-item">
            <a onclick="fn('news')" class="nav-link" href="#">news</a>
          </li>
        </ul>
        <span class="navbar-text">
          (admin pannel)
          <p><%= adminname %></p>
        </span>
      </div>
    </div>
  </nav>


<div class="container-fluid d-flex justify-content-center" >
  <div id="get-music-list" class="container-fluid " > 
  <a  style="  margin-top: 5%; color: black; text-decoration: none;" href="/get-music-list"> get-music-list </a>
  <p> <%= music_list %> </p>
  </div>
  <div id="upload-music" style="  margin-top: 5%; color: black;"  class="  container-fluid " >
      <p>upload music</p>
      <form action="/upload-music" method="post">
      <input placeholder="link" name="link" type="text">
      <input placeholder="name" name="name" type="text">
      <input placeholder="listen_link" name="listen_link" type="text">
      <button type="submit" >upload</button>
    </form>
  </div>

  <div id="delet-music" style="  margin-top: 5%; color: black;"  class="  container-fluid " >
    <p>delet music</p>
    <form action="/delet-music" method="post">
    <input placeholder="id" name="id" type="text">
    <button type="submit" > delete  </button>
  </form>
  </div>

  <div id="upload-picture" style="  margin-top: 5%; color: black;"  class="  container-fluid " >
    <p>uplload picture</p>
    <form action="/upload-picture" method="post">
    <input placeholder="link" name="link" type="text">
    <input placeholder="name" name="name" type="text">
    <button type="submit" >upload</button>
  </form>
</div>

<div id="get-news-list" class="container-fluid " > 
  <!-- <p</p> -->
  <a  style="  margin-top: 5%; color: black; text-decoration: none;" href="/get-news-list"> get-news-list </a>
  <p></p>
  </div>


<div id="upload-news" style="  margin-top: 5%; color: black;"  class="  container-fluid " >
    <p>upload news</p>
    <form action="/upload-news" method="post">
    <input placeholder="content" name="link" type="text">
    <input placeholder="name" name="name" type="text">
    <!-- <input placeholder="listen-link" name="listen-link" type="text"> -->
    <button type="submit" >upload</button>
  </form>
</div>

<div id="get-txt-list" class="container-fluid " > 
  <a  style="  margin-top: 5%; color: black; text-decoration: none;" href="/get-txt-list"> get-txt-list </a>
  <p></p>
  </div>

<div id="upload-txt" style="  margin-top: 5%; color: black;"  class="  container-fluid " >
    <p>upload txt</p>
    <form action="/upload-txt" method="post">
    <input placeholder="content" name="link" type="text">
    <input placeholder="name" name="name" type="text">
    <!-- <input placeholder="listen-link" name="listen-link" type="text"> -->
    <button type="submit" >upload</button>
  </form>
  </div>


</div>




</body>

<script>
 $('#get-music-list').hide()
  $('#get-news-list').hide()
  $('#get-txt-list').hide()
  $('#upload-music').hide()
  $('#upload-txt').hide()
  $('#upload-picture').hide()
  $('#upload-news').hide()
  $('#delet-music').hide()

const hidden = ()=>{
  $('#get-music-list').hide()
  $('#get-news-list').hide()
  $('#get-txt-list').hide()
  $('#upload-music').hide()
  $('#upload-txt').hide()
  $('#upload-picture').hide()
  $('#upload-news').hide()
  $('#delet-music').hide()
}
 


  const fn = (name)=>{
    switch (name) {
      case 'music':
      hidden()
      $('#get-music-list').show()
      $('#upload-music').show()
      $('#delet-music').show()
        break;
      case 'photo':
      hidden()
      $('#upload-picture').show()
      break;

      case 'txt':
      hidden()
      $('#upload-txt').show()
      $('#get-txt-list').show()
      break;

      case 'news':
      hidden()
      $('#get-news-list').show()
      $('#upload-news').show()
      break;
    }
  }




</script>


</html>